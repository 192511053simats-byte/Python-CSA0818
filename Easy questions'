{
	int a,b;
	int *p=&a;
	int *q=&b;
	
	printf("Enter the value a");
	scanf("%d",p);
	printf("Enter the value b");
	scanf("%d",q);	
	
	if(*p>*q)
	{
		printf("a is graeter");
	}
	else
	{
		printf("b is graeter");
	}
}


