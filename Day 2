#Mean Median Average
n = int(input("Enter the number of sequences: "))
l = []

for i in range(n):
    a = int(input("Enter the number: "))
    l.append(a)

#Mean
mean=sum(l)/len(l)
# Median

l_n=sorted(l)
t=len(l_n)
if t%2==0:
   print(l_n[(t+1)//2], l_n[(t-1)//2])
else:
     print(l_n[len(l_n)//2])




        



